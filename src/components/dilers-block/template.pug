mixin dilersBlock(data)
	.dilers-block&attributes(attributes)
		a(href="#" target="_blank")._title
			+svg('diler-title')._icon
			span
				!=data.title
		a(href="")._item
			+svg('diler-location')._icon
			span
				!=data.location
			
		a(href="mailto:")._item
			+svg('diler-email')._icon
			span
				!=data.email
		._items
			a(href="tel: 0000")._item
				+svg('diler-phone')._icon
				span
					!=data.tel1
			a(href="tel: 0000")._item
				+svg('diler-phone')._icon
				span
					!=data.tel2

